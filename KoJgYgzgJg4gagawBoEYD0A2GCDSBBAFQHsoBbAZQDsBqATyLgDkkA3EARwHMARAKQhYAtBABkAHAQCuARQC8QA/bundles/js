var DATE_FORMAT = {
    IND_DATE: "dd-mm-yyyy",
    MONTH_YEARS: "mmm yyyy",
    SQL_SIMPLE_Format : "yyyy-mm-dd",
    MONTH_YEARS_2: "mm-yyyy",
    TIME_SHORT : "HH:MM",
    IND_DATE_TIME_SHORT: "dd-mm-yyyy HH:MM"
};
var DATE_ADD_FORMAT = {
    YEAR:"YEAR",
    MONTH:"MONTH",
    DATE:"DATE",
    HOUR:"HOUR",
    MINUTE:"MINUTE"
};
var MyFormat = {
    strDateChangeFormat: function (strDate, formatSrc, formatDest) {
        var mydate = null;
        if (strDate == null) return "";
        if (formatSrc == DATE_FORMAT.IND_DATE || strDate.length == 10) {
            var parts = strDate.split('-');
            mydate = new Date(parts[2] + '-' + parts[1] + '-' + parts[0] + " 00:00");
        } else if (formatSrc == DATE_FORMAT.IND_DATE_TIME_SHORT || strDate.length == 16) {
            var partsDate = strDate.substr(0, 10).split('-');
            var partsJam = strDate.substr(11, 5).split(':');
            mydate = new Date(partsDate[2] + '-' + partsDate[1] + '-' + partsDate[0] + ' ' + partsJam[0] + ':' + partsJam[1]);
        }
       
        
        return dateFormat(mydate, formatDest);
    },

    strToDate: function (strDate, formatSrc) {
        var mydate = null;
        if (formatSrc == DATE_FORMAT.IND_DATE) {
            var parts = strDate.split('-');
            mydate = new Date(parts[2] + '-' + parts[1] + '-' + parts[0]);
        }
        return mydate
    },

    dateAdd(mydate, formatAdd, i) {
        var temp = new Date(mydate.getTime());
        if (formatAdd == DATE_ADD_FORMAT.DATE) {
            temp.setDate(temp.getDate() + i);
            return temp;
        }
        else if (formatAdd == DATE_ADD_FORMAT.MONTH) {
            temp.setMonth(temp.getMonth() + i);
            return temp;
        }
    },

    dateFormat(mydate, formatDest) {
        return dateFormat(mydate, formatDest);
    },

    indCurrency: function (angka) {
        var rupiah = '';
        var angkarev = angka.toString().split('').reverse().join('');
        for (var i = 0; i < angkarev.length; i++) if (i % 3 == 0) rupiah += angkarev.substr(i, 3) + '.';
        return 'Rp. ' + rupiah.split('', rupiah.length - 1).reverse().join('')+',-';
    }


};
